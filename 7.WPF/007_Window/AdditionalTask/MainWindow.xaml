<Window x:Class="AdditionalTask.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdditionalTask"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontSize="25">
    <Grid>
        <StackPanel Margin="20" Width="Auto" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Name="button1" Content="Window1" Width="Auto" Height="Auto" HorizontalAlignment="Center" Margin="15" Padding="8" Click="button1_Click"></Button>
            <Button Name="button2" Content="Window2" Width="Auto" Height="Auto" HorizontalAlignment="Center" Margin="15" Padding="8" Click="button2_Click"></Button>
            <Button Name="button3" Content="Window3" Width="Auto" Height="Auto" HorizontalAlignment="Center" Margin="15" Padding="8" Click="button3_Click"></Button>
            <Button Name="button4" Content="Window4" Width="Auto" Height="Auto" HorizontalAlignment="Center" Margin="15" Padding="8" Click="button4_Click"></Button>
            <Button Name="button5" Content="Window5" Width="Auto" Height="Auto" HorizontalAlignment="Center" Margin="15" Padding="8" Click="button5_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
